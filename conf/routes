# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                                   controllers.Application.index
GET     /jsonN/:name            		    controllers.Application.jsonN(name: String)
GET     /jsonD/:name                		controllers.Application.jsonD(name: String)

GET     /newQuery                           controllers.SqlQueryContr.newQuery
POST    /querySave             				controllers.SqlQueryContr.save(id:String ?="")


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
